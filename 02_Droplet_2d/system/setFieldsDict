FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    cylinderToCell
    {
        p1 (15 15 -0.5);
        p2 (15 15  0.5);
        radius 4;

        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);

